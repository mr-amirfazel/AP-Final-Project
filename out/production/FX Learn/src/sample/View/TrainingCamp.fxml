<AnchorPane fx:id="TrainingCampPane" prefHeight="644.0" prefWidth="460.0" stylesheets="@StyleSheets/TrainingCamp.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.TrainingCamp">
